package sort;

public class selecsort {
public static void main(String args[])
{
	
}
public static int[] procedure(int a[])
{
	for(int i=0;i<a.length;i++){
		int min=i;
		for(int j=i+1;j<a.length-1;j++)
		{
			if(a[j]<min)
			{
				min=j;
			}
		}
		int temp=a[i];
		a[i]=a[min];
		a[min]=temp;
		
	}
}return a;
}
