package seaching;

import java.util.Scanner;

public class search {
	
	public static void main(String[] args)
	{
		int[] a =new int[5];
		a={2,4,5,6,8};
		Scanner sc=new Scanner(System.in);
		
	}


	public static int procedure(int [] arr,int x)
	{for(int i=0;i<arr.length;i++)
	{	
		if(arr[i]==x)
		{
			return i;
		}
	}
		return -1;
	}
		
		
}
